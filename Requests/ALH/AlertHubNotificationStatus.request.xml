<requestTemplate>
  <type>REST</type>
  <url>{{#AlertHubRestServiceURL}}</url>
  <path>/delivery/api/v1.0/pushnotification/notificationsstatus</path>
  <method>POST</method>
  <headers>
    <header>
      <name>Content-Type</name>
      <value>application/json</value>
    </header>
    <header>
      <name>Authorization</name>
      <value>{{token_Type}} {{access_token}}</value>
    </header>
  </headers>
  <body>
    {
      "deviceNotificationsDto": [{ 
      "appId": "{{appId}}",
      "deviceType": "{{deviceType}}",
      "notificationId": "{{notificationId}}" }]
      }
  </body>
  <responseActions>
    <action>
      <path>status</path>
      <display>true</display>
      <globalVariableName>status</globalVariableName>
    </action>
    <action>
      <path>notificationId</path>
      <display>true</display>
      <globalVariableName>notificationId</globalVariableName>
    </action>
    <action>
      <path>.</path>
      <display>true</display>
    </action>
  </responseActions>
</requestTemplate>
